#include<bits/stdc++.h>
using namespace std;
int main()
{
	int t;
	cin>>t;
	while(t--)
	{
		int n,i=0,d=1;
		long long int c;
		cin>>n;cin>>c;
		int a[n];
		long long int sum=0;
		for(i=0;i<n;i++)
		cin>>a[i];
		for(i=0;i<n;i++)
		{	
			sum+=a[i];
			if(sum>c)
			{
				cout<<"No"<<endl;
				d=0;
				break;
			}
		}
		if(d==1)
		cout<<"Yes"<<endl;
	}
}
			
			
